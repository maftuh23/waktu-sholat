{
  "version": 2,
  "installCommand": "yarn install --ignore-scripts",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/app.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=86400, stale-while-revalidate"
        }
      ]
    }
  ]
}